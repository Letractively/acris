<?xml version="1.0" encoding="UTF-8"?>

<module>

	<!-- Gilead dependencies -->
	<inherits name='net.sf.gilead.Gilead4Gwt'/>
	<inherits name='net.sf.gilead.emul.java5.ejb3.Ejb3'/>

	<source path="gilead.client" />
	<source path="gilead.rpc" />

	<!-- Gilead dynamic proxy classes -->
	<generate-with class="net.sf.gilead.proxy.gwt.Gwt15ProxyGenerator">
		<any>
			<when-type-is class="sk.seges.acris.security.shared.user_management.domain.gilead.GileadGenericUser" />
			<when-type-is class="sk.seges.acris.security.shared.user_management.domain.gilead.GileadRolePermission" />
			<when-type-is class="sk.seges.acris.security.shared.user_management.domain.gilead.GileadSecurityPermission" />
			<when-type-is class="sk.seges.acris.security.shared.user_management.domain.gilead.GileadSecurityRole" />
			<when-type-is class="sk.seges.acris.security.shared.user_management.domain.gilead.GileadUserPreferences" />
			<when-type-is class="sk.seges.acris.security.shared.user_management.domain.gilead.GileadUserWithAuthorities" />
		</any>
	</generate-with>

	<generate-with class="net.sf.gilead.proxy.gwt.Gwt15ProxyGenerator">
		<when-type-assignable class="sk.seges.acris.security.shared.domain.ITransferableObject" />
	</generate-with>

</module>