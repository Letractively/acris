<!-- MoviePanel.ui.xml -->
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <g:HTMLPanel>
	<div class="chocolatePanel ifadvert">
		<div>
			<div class="chocolatePanel tops">
				<div>
					<div class="chocolatePanel user1">
						<div class="inside">
							<div class="yjsquare">
								<div class="container">
									<g:Image ui:field="picture" width="110px" height="70px" styleName="leftblock"></g:Image>
									<div class="leftblock">
										<div class="chocolatePanel heading">
											<h1><g:HTMLPanel ui:field="heading"></g:HTMLPanel></h1>
										</div>
										<div class="chocolatePanel yjsquare_in">
											<g:HTMLPanel ui:field="text"></g:HTMLPanel>
										</div>
										<g:HorizontalPanel ui:field="rating"></g:HorizontalPanel>
									</div>
                            		<div class="acris-cleaner"></div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
 </g:HTMLPanel>
</ui:UiBinder>