<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                           http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-2.0.xsd">

	<bean name="securityRoleGileadService"
		class="sk.seges.acris.rpc.GileadGWTCustomPolicyRPCServiceExporter">
		<property name="beanManager" ref="acrisSecurityBeanManager" />
		<property name="serializationPolicy" ref="remoteContextSerializationPolicy" />
		<property name="service" ref="securityRoleService" />
		<property name="serviceInterfaces">
			<value>sk.seges.acris.security.shared.user_management.service.IRoleService</value>
		</property>
	</bean>

</beans>