<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:s="urn:import:sk.seges.acris.theme.client">

	<div class="acris-metal-dialog">
		<div tabindex="0" class="acris-metal-dialog-header" ui:field="header">
			<div class="acris-metal-dialog-top-inner">
				<div class="acris-metal-dialog-header-top">
					<div class="acris-metal-dialog-top-left-corner"></div>
					<div class="acris-metal-dialog-top-right-corner"></div>
					<div class="acris-cmp-cleaner"></div>
				</div>	
				<div class="acris-metal-dialog-header-content">
					<div class="acris-metal-dialog-header-content-text">
						<div ui:field="caption"></div>
					</div>
					<div class="acris-metal-dialog-close-button">
						<div ui:field="close"></div>
					</div>
					<div class="acris-cmp-cleaner"></div>
				</div>
			</div>
		</div>
		<div class="acris-metal-dialog-main">
			<div class="acris-metal-dialog-main-text" ui:field="dialogBox"></div>
		</div>
		<div class="acris-metal-dialog-footer">
			<div class="acris-metal-dialog-footer-inner">
				<div class="acris-metal-dialog-footer-bottom">
					<div class="acris-metal-dialog-bottom-left-corner"></div>
					<div class="acris-metal-dialog-bottom-right-corner"></div>
					<div class="acris-cmp-cleaner"></div>
				</div>
			</div>
		</div>
	</div>
</ui:UiBinder>