<module>
	<inherits name='sk.seges.acris.widget.Widgets'/>
	<inherits name='com.google.gwt.gen2.table.Table'/>
	<inherits name='com.google.gwt.gen2.picker.Picker' />
	<inherits name='sk.seges.sesam.Core' />
	<inherits name='sk.seges.acris.Callbacks' />
	
	<generate-with
		class="sk.seges.acris.widget.rebind.table.BeanTableSpecGenerator">
		<when-type-assignable
			class="sk.seges.acris.widget.client.table.BeanTableSpec"/>
	</generate-with>
	
    <generate-with
        class="sk.seges.acris.widget.rebind.filterpanel.ConjunctiveFilterPanelSpecGenerator">
        <when-type-assignable
            class="sk.seges.acris.widget.client.filterpanel.FilterPanelSpec"/>
    </generate-with>
    <inherits name="com.google.gwt.gen2.picker.Picker"/>
</module>